{
  "name": "benaton",
  "private": true,
  "version": "0.0.1",
  "author": "Masahiko Sakakibara<sakakibara@rdlabo.jp>",
  "description": "西宮・夙川のフレンチレストラン「ル ベナトン」のWebサイト",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "fmt": "npm run fmt:typescript && npm run fmt:css",
    "fmt:css": "prettier --parser scss --write \"src/**/*.scss\"",
    "fmt:typescript": "prettier --parser typescript --write \"src/**/*.ts\"",
    "deploy": "npm run build -- --prerender && firebase deploy -f"
  },
  "devDependencies": {
    "@stencil/core": "^2.16.1",
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^26.0.24",
    "firebase-tools": "^9.23.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.5.1",
    "puppeteer": "^10.0.0",
    "stencil-router-v2": "^0.6.0"
  },
  "license": "MIT",
  "dependencies": {
    "firebase": "^9.6.0"
  }
}
