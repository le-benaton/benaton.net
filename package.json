{
  "name": "benaton",
  "private": true,
  "version": "0.0.1",
  "author": "Masahiko Sakakibara<sakakibara@rdlabo.jp>",
  "description": "西宮・夙川のフレンチレストラン「ル ベナトン」のWebサイト",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "fmt": "npm run fmt:typescript && npm run fmt:css",
    "fmt:css": "prettier --parser scss --write \"src/**/*.scss\"",
    "fmt:typescript": "prettier --parser typescript --write \"src/**/*.ts\"",
    "deploy": "npm run build -- --prerender && firebase deploy -f"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@stencil/sass": "^3.0.7",
    "@types/jest": "^29.5.6",
    "firebase-tools": "^12.9.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^2.5.1",
    "puppeteer": "^21.1.1"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16.18.11",
    "firebase": "^10.7.0",
    "stencil-router-v2": "^0.6.0"
  }
}
